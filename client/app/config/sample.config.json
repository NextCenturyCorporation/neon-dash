{
    "routeService": {
        "url": "http://localhost:8090/ghREST/",
        "get": "route?lat1=LAT1&lon1=LON1&lat2=LAT2&lon2=LON2",
        "post": "dynamicroute"
    },

    "user_ale": {
        "loggingUrl": "http://192.168.1.100",
        "toolName": "Neon Dashboard",
        "toolVersion": "1.2.0",
        "elementGroups": [
            "top",
            "map_group",
            "table_group",
            "chart_group",
            "query_group",
            "graph_group"
        ],
        "workerUrl": "bower_components/user-ale/js/userale-worker.js",
        "debug": false,
        "sendLogs": false
    },

    "dashboard": {
        "gridsterColumns": 24,
        "gridsterMargins": 10,
        "hideNavbarItems": false,
        "hideAdvancedOptions": false,
        "hideFilterStatusTray": false,
        "hideErrorNotifications": false,
        "hideHeaders": false,
        "hideCloseButton": false,
        "showVideoOnLoad": false,
        "showImport": false,
        "showExport": true,
        "translationKeys": {
            "google": "API_KEY"
        }
    },

    "help": {
        "guide": "./app/help/Neon-Dashboard-User-Guide.pdf",
        "webVideo": "",
        "localVideo": ""
    },

    "opencpu": {
        "enableOpenCpu": false,
        "enableLogging": false,
        "useAlerts": false,
        "enableAnomalyDetection": true,
        "enableStl2": false,
        "enableMmpp": false,
        "url": "http://neon-opencpu/ocpu/library/NeonAngularDemo/R"
    },

    "externalServices": {
        "jsonConfigFile": "",
        "appImagePath": "",
        "fileProps": {
            "url": "url",
            "name": "name",
            "image": "image",
            "services": "services"
        },
        "servicesMappings": {
            "neonMapping1": "serviceName1",
            "neonMapping2": "serviceName2"
        },
        "argsMappings": {
            "neonMapping1": {
                "serviceName1": "neonMapping1"
            },
            "neonMapping2": {
                "serviceName2": {
                    "propertyName1": "neonMapping3",
                    "propertyName2": "neonMapping4"
                }
            }
        }
    },

    "datasets": [{
        "name": "Earthquakes",
        "datastore": "mongo",
        "hostname": "localhost",
        "databases": [{
            "name": "test",
            "prettyName": "Test",
            "tables": [{
                "name": "earthquakes",
                "prettyName": "Earthquakes",
                "fields": [{
                    "columnName": "_id",
                    "prettyName": "Database ID"
                }, {
                    "columnName": "time",
                    "prettyName": "Time"
                }, {
                    "columnName": "latitude",
                    "prettyName": "Latitude"
                }, {
                    "columnName": "longitude",
                    "prettyName": "Longitude"
                }, {
                    "columnName": "depth",
                    "prettyName": "Depth"
                }, {
                    "columnName": "mag",
                    "prettyName": "Magnitude"
                }, {
                    "columnName": "magType",
                    "prettyName": "Magnitude Type"
                }, {
                    "columnName": "nst",
                    "prettyName": "NST"
                }, {
                    "columnName": "gap",
                    "prettyName": "Gap"
                }, {
                    "columnName": "dmin",
                    "prettyName": "DMIN"
                }, {
                    "columnName": "rms",
                    "prettyName": "RMS"
                }, {
                    "columnName": "net",
                    "prettyName": "Net"
                }, {
                    "columnName": "id",
                    "prettyName": "ID"
                }, {
                    "columnName": "updated",
                    "prettyName": "Updated"
                }, {
                    "columnName": "place",
                    "prettyName": "Place"
                }, {
                    "columnName": "type",
                    "prettyName": "Type"
                }],
                "mappings": {
                    "date": "time",
                    "latitude": "latitude",
                    "longitude": "longitude",
                    "line_category": "type",
                    "bar_x_axis": "net",
                    "y_axis": "mag",
                    "color_by": "type",
                    "size_by": "mag",
                    "sort_by": "time",
                    "count_by": "net"
                }
            }
            ]
        }],
        "mapConfig": {
            "myMap1": {
                "bounds": {
                    "left": -180,
                    "bottom": -90,
                    "right": 180,
                    "top": 90
                }
            }
        },
        "linkyConfig": {
            "mentions": false,
            "hashtags": false,
            "urls": true,
            "linkTo": "twitter"
        },
        "options": {
            "colorMaps": {
                "test": {
                    "earthquakes": {
                        "myField": {
                            "fieldA": "",
                            "fieldB": ""
                        }
                    }
                }
            },
            "mapBaseLayer": {
                "color": "dark",
                "protocol": "https"
            }
        }
    }],

    "layouts": {
        "default": [{
            "sizeX": 12,
            "type": "timeline"
        }, {
            "sizeX": 12,
            "type": "map",
            "bindings": {
                "config": [
                {
                    "database": "test",
                    "table": "earthquakes"
                }
                ]
            }
        }, {
            "sizeX": 12,
            "type": "lineChart",
            "bindings": {
                "config": [
                {
                    "database": "test",
                    "table": "earthquakes"
                }
                ]
            }
        }, {
            "type": "aggregationTable"
        }, {
            "type": "barChart"
        }, {
            "type": "opsClock"
        }, {
            "type": "dataTable"
        }, {
            "type": "filterBuilder"
        }],

        "example-with-bindings": [{
            "sizeX": 12,
            "type": "timeline",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "dateField": "time",
                "granularity": "month",
                "overrideStartDate": "1/1/2013",
                "overrideEndDate": "1388534399000"
            }
        }, {
            "sizeX": 12,
            "type": "map",
            "bindings": {
                "title": "myTitle",
                "config": [{
                    "name": "myLayer",
                    "database": "test",
                    "table": "earthquakes",
                    "type": "points",
                    "limit": 5000,
                    "latitudeField": "latitude",
                    "longitudeMapping": "longitude",
                    "dateField": "time",
                    "sizeField": "mag",
                    "colorField": "type"
                }, {
                    "name": "myLayer",
                    "database": "test",
                    "table": "earthquakes",
                    "type": "heatmap",
                    "limit": 5000,
                    "latitudeField": "latitude",
                    "longitudeField": "longitude"
                }, {
                    "name": "myLayer",
                    "database": "test",
                    "table": "earthquakes",
                    "type": "cluster",
                    "limit": 5000,
                    "latitudeField": "latitude",
                    "longitudeMapping": "longitude",
                    "dateField": "time",
                    "sizeField": "mag",
                    "colorCode": "#39b54a",
                    "popupFields": [
                        "net"
                        ]
                }]
            }
        }, {
            "sizeX": 12,
            "type": "lineChart",
            "bindings": {
                "title": "myTitle",
                "granularity": "day",
                "config": [{
                    "name": "myLayer",
                    "database": "test",
                    "table": "earthquakes",
                    "dateField": "time",
                    "aggregationType": "count"
                }, {
                    "name": "myLayer",
                    "database": "test",
                    "table": "earthquakes",
                    "dateField": "time",
                    "aggregationType": "average",
                    "aggregationField": "mag",
                    "groupField": "type"
                }]
            }
        }, {
            "sizeX": 12,
            "type": "dataTable",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "idField": "_id",
                "sortDirection": 1,
                "limit": 500
            }
        }, {
            "sizeX": 6,
            "type": "aggregationTable",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "groupField": "net",
                "aggregationType": "max",
                "aggregationField": "mag",
                "limit": 100
            }
        }, {
            "sizeX": 6,
            "type": "barChart",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "groupField": "net",
                "aggregationType": "average",
                "aggregationField": "mag",
                "limit": 50
            }
        }, {
            "sizeX": 6,
            "type": "newsFeed",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "primaryTitleField": "type",
                "secondaryTitleField": "net",
                "dateField": "time",
                "contentField": "mag"
            }
        }, {
            "sizeX": 6,
            "type": "opsClock",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "dateField": "time"
            }
        }, {
            "sizeX": 6,
            "type": "scatterPlot",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "type": "scatter",
                "xAxisField": "depth",
                "yAxisField": "mag",
                "limit": "time"
            }
        }, {
            "sizeX": 6,
            "type": "sunburstChart",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "groupFields": "type1,type2,type3",
                "valueField": "type",
                "arcValue": "count"
            }
        }, {
            "sizeX": 6,
            "type": "textCloud",
            "bindings": {
                "title": "myTitle",
                "database": "test",
                "table": "earthquakes",
                "dataField": "net"
            }
        }]
    },

    "legends": {
