  "scripts": {
    "start": "npx ng serve --proxy-config proxy.conf.json  --host 0.0.0.0 --port 4200",
    "build": "./scripts/generate_environment.sh && npx ng build --aot --base-href=/$npm_config_base --deploy-url=/$npm_config_base -c custom",
    "build-prod": "./scripts/generate_environment.sh && npx ng build --aot --base-href=/$npm_config_base --deploy-url=/$npm_config_base --prod",
    "build-war": "./scripts/generate_environment.sh && ./scripts/build_dashboard.sh",
    "lint": "./scripts/lint.sh",
    "test": "npx ng test && npm run lint",
    "e2e": "npx ng e2e",
    "docs": "npx typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "generate-attribution": "node scripts/generate_attributions.js",
    "postinstall": "npm run generate-attribution 1> /dev/null",
    "ensure-attribution": "./scripts/ensure_attributions_file.sh"
  },
  "pre-commit": [
    "ensure-attribution"
  ],
