{
  "name": "neon-gtd",
  "version": "1.3.0-Angular2",
  "license": "Apache2",
  "scripts": {
    "ng": "ng",
    "start": "./scripts/generate_environment.sh && ng serve --proxy-config proxy.conf.json  --host 0.0.0.0 --port 4200",
    "build": "./scripts/generate_environment.sh && ./scripts/build_dashboard.sh",
    "build-prod": "ng build --prod --build-optimizer=false",
    "start-local": "npm start",
    "build-lorelei": "npm run build lorelei",
    "build-hr": "npm run build lorelei-hr",
    "lint": "npm run-script lint-ts && npm run-script lint-html && npm run-script lint-sass",
    "lint-html": "./lint_html.sh",
    "lint-sass": "sass-lint-auto-fix --config sass-lint-auto-fix.yaml --config-sass-lint .sass-lint.yml && sass-lint --config .sass-lint.yml -v",
    "lint-scss": "npm run-script lint-sass",
    "lint-ts": "eslint src/app --cache --ext .js,.ts --fix --max-warnings 0",
    "test": "npm run-script unit-test && npm run-script lint",
    "unit-test": "./scripts/generate_environment.sh && ng test",
    "e2e": "ng e2e",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "typedoc": "typedoc",
    "remove-postcss-custom-properties": "sed -i -E '/customProperties/s/^.*$//g' node_modules/@angular/cli/models/webpack-configs/styles.js",
    "generate-attribution": "node scripts/generate_attributions.js",
    "postinstall": "npm run generate-attribution 1> /dev/null",
    "ensure-attribution": "./scripts/ensure_attributions_file.sh"
  },
  "pre-commit": [
    "ensure-attribution"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.15",
    "@angular/cdk": "~7.3.7",
