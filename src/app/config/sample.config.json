{
    "dashboard": {
        "gridsterColumns": 24,
        "gridsterMargins": 10,
        "hideNavbarItems": false,
        "hideAdvancedOptions": false,
        "hideFilterStatusTray": false,
        "hideErrorNotifications": false,
        "hideHeaders": false,
        "hideCloseButton": false,
        "showVideoOnLoad": false,
        "showImport": false,
        "showExport": true,
        "translationKeys": {
            "google": "API_KEY"
        }
    },

    "help": {
        "guide": "./app/help/Neon-Dashboard-User-Guide.pdf",
        "webVideo": "",
        "localVideo": ""
    },

    "externalServices": {
        "jsonConfigFile": "",
        "appImagePath": "",
        "fileProps": {
            "url": "url",
            "name": "name",
            "image": "image",
            "services": "services"
        },
        "servicesMappings": {
            "neonMapping1": "serviceName1",
            "neonMapping2": "serviceName2"
        },
        "argsMappings": {
            "neonMapping1": {
                "serviceName1": "neonMapping1"
            },
            "neonMapping2": {
                "serviceName2": {
                    "propertyName1": "neonMapping3",
                    "propertyName2": "neonMapping4"
                }
            }
        }
    },

    "datasets": [{
        "name": "Earthquakes",
        "datastore": "mongo",
        "hostname": "localhost",
        "databases": [{
            "name": "test",
            "prettyName": "Test",
            "tables": [{
                "name": "earthquakes",
                "prettyName": "Earthquakes",
                "fields": [{
                    "columnName": "_id",
                    "prettyName": "Database ID"
                }, {
                    "columnName": "time",
                    "prettyName": "Time"
                }, {
                    "columnName": "latitude",
                    "prettyName": "Latitude"
                }, {
                    "columnName": "longitude",
                    "prettyName": "Longitude"
                }, {
                    "columnName": "depth",
                    "prettyName": "Depth"
                }, {
                    "columnName": "mag",
                    "prettyName": "Magnitude"
                }, {
                    "columnName": "magType",
                    "prettyName": "Magnitude Type"
                }, {
                    "columnName": "nst",
                    "prettyName": "NST"
                }, {
                    "columnName": "gap",
                    "prettyName": "Gap"
                }, {
                    "columnName": "dmin",
                    "prettyName": "DMIN"
                }, {
                    "columnName": "rms",
                    "prettyName": "RMS"
                }, {
                    "columnName": "net",
                    "prettyName": "Net"
                }, {
                    "columnName": "id",
                    "prettyName": "ID"
                }, {
                    "columnName": "updated",
                    "prettyName": "Updated"
                }, {
                    "columnName": "place",
                    "prettyName": "Place"
                }, {
                    "columnName": "type",
                    "prettyName": "Type"
                }],
                "mappings": {
                    "date": "time",
                    "latitude": "latitude",
                    "longitude": "longitude",
                    "id": "_id"
                }
            }]
        }],
        "mapConfig": {
            "myMap1": {
                "bounds": {
                    "left": -180,
                    "bottom": -90,
                    "right": 180,
                    "top": 90
                }
            }
        },
        "linkyConfig": {
            "mentions": false,
            "hashtags": false,
            "urls": true,
            "linkTo": "twitter"
        },
        "options": {
            "colorMaps": {
                "test": {
                    "earthquakes": {
                        "myField": {
                            "fieldA": "",
                            "fieldB": ""
                        }
                    }
                }
            },
            "mapBaseLayer": {
                "color": "dark",
                "protocol": "https"
            }
        }
    }],

    "layouts": {
        "default": [
            {
                "sizex": 20,
                "sizey": 6,
                "type": "timeline",
                "bindings": {
                    "database": "test",
                    "table": "earthquakes",
                    "dateField": "time"
                }
            },
            {
                "sizex": 20,
                "sizey": 8,
                "type": "scatterPlot",
                "bindings": {
                    "database": "test",
                    "table": "earthquakes",
                    "xField": "longitude",
                    "yField": "latitude",
                    "limit": 200
                }
            },
            {
                "sizex": 20,
                "sizey": 8,
                "type": "map",
                "bindings": {
                    "database": "test",
                    "table": "earthquakes",
                    "latitudeField": "latitude",
                    "longitudeField": "longitude",
                    "dateField": "time",
                    "sizeField": "mag",
                    "colorField": "magType",
                    "limit": 1000

                }
            },
            {
                "sizex": 20,
                "sizey": 8,
                "type": "map",
                "bindings": {
                    "limit": 1000,
                    "layers": [
                        {
                            "title": "MagType",
                            "database": "test",
                            "table": "earthquakes",
                            "latitudeField": "latitude",
                            "longitudeField": "longitude",
                            "dateField": "time",
                            "sizeField": "mag",
                            "colorField": "magType"
                        }, {
                            "title": "Net",
                            "database": "test",
                            "table": "earthquakes",
                            "latitudeField": "latitude",
                            "longitudeField": "longitude",
                            "colorField": "net",
                            "sizeField": "mag",
                            "dateField": "date"
                        }
                    ]
                }
            },
            {
                "sizex": 20,
                "sizey": 6,
                "type": "barChart",
                "bindings": {
                    "database": "test",
                    "table": "earthquakes",
                    "dataField": "magType",
                    "aggregation": "count"
                }
            },
            {
                "sizex": 20,
                "sizey": 6,
                "type": "lineChart",
                "bindings": {
                    "database": "test",
                    "table": "earthquakes",
                    "dateField": "time",
                    "groupField": "type",
                    "aggregation": "count"
                }
            },
            {
                "sizex": 20,
                "sizey": 6,
                "type": "dataTable",
                "bindings": {
                    "database": "test",
                    "table": "earthquakes",
                    "sortField": "time",
                    "limit": 20
                }
            },
            {
                "sizex": 20,
                "sizey": 4,
                "type": "textCloud",
                "bindings": {
                    "database": "test",
                    "table": "earthquakes",
                    "dataField": "magType"
                }
            },
            {
                "sizex": 20,
                "sizey": 4,
                "type": "filterBuilder",
                "bindings": {
                    "database": "test",
                    "table": "earthquakes"
                }
            }
        ]
    },

    "customFilters": {
        "test": {
            "earthquakes": [{
                "label": "Types",
                "items": [{
                    "label": "Earthquake",
                    "field": "type",
                    "value": "earthquake"
                }, {
                    "label": "Explosion",
                    "field": "type",
                    "value": "explosion"
                }, {
                    "label": "Landslide",
                    "field": "type",
                    "value": "landslide"
                }, {
                    "label": "Mining Explosion",
                    "field": "type",
                    "value": "mining_explosion"
                }, {
                    "label": "Quarry",
                    "field": "type",
                    "value": "quarry"
                }, {
                    "label": "Quarry Blast",
                    "field": "type",
                    "value": "quarry_blast"
                }, {
                    "label": "Sonicboom",
                    "field": "type",
                    "value": "sonicboom"
                }]
            }, {
                "label": "Networks",
                "input": {
                    "field": "net"
                }
            }],
            "myTable": [{
                "label": "My Group",
                "items": [{
                    "label": "Equals My Value",
                    "field": "myField",
                    "value": "myValue"
                }, {
                    "label": "Contains My Value",
                    "field": "myField",
                    "operator": "contains",
                    "value": "myValue"
                }, {
                    "label": "Not Null",
                    "field": "myField"
                }],
                "input": {
                    "field": "myField",
                    "operator": "contains"
                }
            }, {
                "label": "My Group 2",
                "multi": {
                    "myField": [{
                        "value": "A"
                    }, {
                        "operator": "contains",
                        "value": "B"
                    }, {
                        "value": ["C", "D"]
                    }]
                }
            }]
        }
    },

    "files": {
        "datasets": [
            "./app/config/sample.dataset.config.json"
        ],
        "layouts": [
            "./app/config/sample.layout.config.json"
        ]
    }
}
