projectIcon: 'assets/images/verdi-favicon.ico'
datastores:
    es1:
        host: localhost
        type: elasticsearch
        databases:
            kb-events:
                prettyName: Events
                tables:
                    seedling:
                        prettyName: Seedling
                        fields:
                        labelOptions:
            kb-geo:
                prettyName: Geolocation
                
                tables:
                    seedling:
                        prettyName: Seedling
                        fields:
                        labelOptions:
            kb-simple:
                prettyName: Network
                tables:
                    seedling:
                        prettyName: Seedling
                        fields:
                        labelOptions:
            verdi-rdf:
                prettyName: Documents
                tables:
                    normal:
                        prettyName: Normal
                        fields:
                        labelOptions:
                    kb:
                      prettyName: Knowledge Base
                      fields:
                      labelOptions:
                    reified:
                        prettyName: Reified
                        fields:
                        labelOptions:
            verdi-support:
                prettyName: Support
                tables:
                    hypothesis_info:
                        prettyName: Hypotheses
                        fields:
                        labelOptions:
                    parent_children:
                        prettyName: Parent Children Documents
                        fields:
                        labelOptions:
            rpi-m9-ta1-v2:
                prettyName: TA1
                tables:
                    graph:
                        prettyName: Documents
                        fields:
                        labelOptions:
                    docs:
                         prettyName: Root Documents
                         fields:
                         labelOptions:
                    children:
                         prettyName: Child Documents
                         fields:
                         labelOptions:
            rpi-m9-ta1-v2-graph:
                prettyName: TA2 Network
                tables:
                    graph:
                        prettyName: ARL M9 Graph
                        fields:
                        labelOptions:
            rpi-m9-ta1-v2-events:
                prettyName: TA2 Events
                tables:
                    graph:
                        prettyName: ARL M9 Events
                        fields:
                        labelOptions:
            rpi-m9-ta1-v2-geo:
                 prettyName: TA2 Locations
                 tables:
                     graph:
                         prettyName: ARL M9 Geonames
                         fields:
                         labelOptions:
            aida-docs-tmp:
                prettyName: Documents
                tables:
                    parents:
                        prettyName: Root Documents
                        fields:
                        labelOptions:
                    parent_children:
                         prettyName: Child Documents
                         fields:
                         labelOptions:
            gaia:
                prettyName: GAIA
                tables:
                    graph:
                        prettyName: Documents
                        fields:
                        labelOptions:

