projectIcon: "assets/images/verdi-favicon.ico"
datastores:
    es1:
        host: localhost
        type: elasticsearch
        databases:
            kb-events:
                prettyName: Events
                tables:
                    seedling:
                        prettyName: Seedling
                        fields:
                        labelOptions:
            kb-geo:
                prettyName: Geolocation
                tables:
                    seedling:
                        prettyName: Seedling
                        fields:
                        labelOptions:
            kb-simple:
                prettyName: Network
                tables:
                    seedling:
                        prettyName: Seedling
                        fields:
                        labelOptions:
            verdi-rdf:
                prettyName: Documents
                tables:
                    normal:
                        prettyName: Normal
                        fields:
                        labelOptions:
                    kb:
                      prettyName: Knowledge Base
                      fields:
                      labelOptions:
                    reified:
                        prettyName: Reified
                        fields:
                        labelOptions:
            verdi-support:
                prettyName: Support
                tables:
                    hypothesis_info:
                        prettyName: Hypotheses
                        fields:
                        labelOptions:
                    parent_children:
                        prettyName: Parent Children Documents
                        fields:
                        labelOptions:

