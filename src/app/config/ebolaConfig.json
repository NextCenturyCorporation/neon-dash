{
    "dashboard": {
        "gridsterColumns": 24,
        "gridsterMargins": 10,
        "hideNavbarItems": false,
        "hideAdvancedOptions": false,
        "hideFilterStatusTray": false,
        "hideErrorNotifications": false,
        "hideHeaders": false,
        "hideCloseButton": false,
        "showVideoOnLoad": false,
        "showImport": false,
        "showExport": true,
        "translationKeys": {
            "google": "API_KEy"
        }
    },

    "help": {
        "guide": "./app/help/Neon-Dashboard-User-Guide.pdf",
        "webVideo": "",
        "localVideo": ""
    },

    "externalServices": {
        "jsonConfigFile": "",
        "appImagePath": "",
        "fileProps": {
            "url": "url",
            "name": "name",
            "image": "image",
            "services": "services"
        },
        "servicesMappings": {
            "neonMapping1": "serviceName1",
            "neonMapping2": "serviceName2"
        },
        "argsMappings": {
            "neonMapping1": {
                "serviceName1": "neonMapping1"
            },
            "neonMapping2": {
                "serviceName2": {
                    "propertyName1": "neonMapping3",
                    "propertyName2": "neonMapping4"
                }
            }
        }
    },

    "datasets": [{
        "name": "ebola_data",
        "datastore": "elasticsearch",
        "hostname": "localhost",
        "databases": [{
            "name": "ebola_data",
            "prettyName": "ebola_data",
            "tables": [{
                "name": "record",
                "prettyName": "record",
                "fields": [{
                    "columnName": "_id",
                    "prettyName": "Database ID"
                }, {
                    "columnName": "time",
                    "prettyName": "Time"
                }, {
                    "columnName": "latitude",
                    "prettyName": "Latitude"
                }, {
                    "columnName": "longitude",
                    "prettyName": "Longitude"
                }, {
                    "columnName": "depth",
                    "prettyName": "Depth"
                }, {
                    "columnName": "mag",
                    "prettyName": "Magnitude"
                }, {
                    "columnName": "magType",
                    "prettyName": "Magnitude Type"
                }, {
                    "columnName": "nst",
                    "prettyName": "NST"
                }, {
                    "columnName": "gap",
                    "prettyName": "Gap"
                }, {
                    "columnName": "dmin",
                    "prettyName": "DMIN"
                }, {
                    "columnName": "rms",
                    "prettyName": "RMS"
                }, {
                    "columnName": "net",
                    "prettyName": "Net"
                }, {
                    "columnName": "id",
                    "prettyName": "ID"
                }, {
                    "columnName": "updated",
                    "prettyName": "Updated"
                }, {
                    "columnName": "place",
                    "prettyName": "Place"
                }, {
                    "columnName": "type",
                    "prettyName": "Type"
                }],
                "mappings": {
                    "date": "time",
                    "latitude": "latitude",
                    "longitude": "longitude",
                    "line_category": "type",
                    "bar_x_axis": "net",
                    "y_axis": "mag",
                    "color_by": "type",
                    "size_by": "mag",
                    "sort_by": "time",
                    "count_by": "net"
                }
            }]
        }],
        "mapConfig": {
            "myMap1": {
                "bounds": {
                    "left": -180,
                    "bottom": -90,
                    "right": 180,
                    "top": 90
                }
            }
        },
        "linkyConfig": {
            "mentions": false,
            "hashtags": false,
            "urls": true,
            "linkTo": "twitter"
        },
        "options": {
            "colorMaps": {
                "record": {
                    "ebola_data": {
                        "myField": {
                            "fieldA": "",
                            "fieldB": ""
                        }
                    }
                }
            },
            "mapBaseLayer": {
                "color": "dark",
                "protocol": "https"
            }
        }
    }],

    "layouts": {
        "default": [{
                "sizex": 10,
                "sizey": 6,
                "type": "filterBuilder",
                "bindings": {
                    "database": "record",
                    "table": "ebola_data"
                }
            },
            {
                "sizex": 10,
                "sizey": 6,
                "type": "scatterPlot",
                "bindings": {
                    "database": "record",
                    "table": "ebola_data",
                    "xField": "longitude",
                    "yField": "latitude",
                    "limit": 200
                }
            },
            {
                "sizex": 10,
                "sizey": 6,
                "type": "dataTable",
                "bindings": {
                    "database": "record",
                    "table": "ebola_data",
                    "sortField": "time",
                    "limit": 20
                }
            },
            {
                "sizex": 8,
                "sizey": 8,
                "type": "map",
                "bindings": {
                    "database": "record",
                    "table": "ebola_data",
                    "latitudeField": "latitude",
                    "longitudeField": "longitude",
                    "dateField": "time",
                    "sizeField": "mag",
                    "colorField": "magType",
                    "limit": 1000

                }
            },
            {
                "sizex": 6,
                "sizey": 4,
                "type": "barChart",
                "bindings": {
                    "database": "record",
                    "table": "ebola_data",
                    "dataField": "magType",
                    "aggregation": "count"
                }
            },
            {
                "sizex": 6,
                "sizey": 4,
                "type": "lineChart",
                "bindings": {
                    "database": "ebola_data",
                    "table": "record",
                    "dateField": "time",
                    "groupField": "type",
                    "aggregation": "count"
                }
            },
            {
                "sizex": 10,
                "sizey": 6,
                "type": "textCloud",
                "bindings": {
                    "database": "record",
                    "table": "ebola_data",
                    "dataField": "situationFrame.entities"
                }
            },
            {
                "sizex": 6,
                "sizey": 4,
                "type": "timeline",
                "bindings": {
                    "database": "record",
                    "table": "ebola_data",
                    "dateField": "time"
                }
            }
        ],

        "example-with-bindings": [{
            "sizex": 6,
            "type": "timeline",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "dateField": "time",
                "granularity": "month",
                "overrideStartDate": "1/1/2013",
                "overrideEndDate": "1388534399000"
            }
        }, {
            "sizex": 6,
            "type": "map",
            "bindings": {
                "title": "myTitle",
                "config": [{
                    "name": "myLayer",
                    "database": "record",
                    "table": "ebola_data",
                    "type": "points",
                    "limit": 5000,
                    "latitudeField": "latitude",
                    "longitudeMapping": "longitude",
                    "dateField": "time",
                    "sizeField": "mag",
                    "colorField": "type"
                }, {
                    "name": "myLayer",
                    "database": "record",
                    "table": "ebola_data",
                    "type": "heatmap",
                    "limit": 5000,
                    "latitudeField": "latitude",
                    "longitudeField": "longitude"
                }, {
                    "name": "myLayer",
                    "database": "record",
                    "table": "ebola_data",
                    "type": "cluster",
                    "limit": 5000,
                    "latitudeField": "latitude",
                    "longitudeMapping": "longitude",
                    "dateField": "time",
                    "sizeField": "mag",
                    "colorCode": "#39b54a",
                    "popupFields": [
                        "net"
                    ]
                }]
            }
        }, {
            "sizex": 6,
            "type": "lineChart",
            "bindings": {
                "title": "myTitle",
                "granularity": "day",
                "config": [{
                    "name": "myLayer",
                    "database": "record",
                    "table": "ebola_data",
                    "dateField": "time",
                    "aggregationType": "count"
                }, {
                    "name": "myLayer",
                    "database": "record",
                    "table": "ebola_data",
                    "dateField": "time",
                    "aggregationType": "average",
                    "aggregationField": "mag",
                    "groupField": "type"
                }]
            }
        }, {
            "sizex": 6,
            "type": "dataTable",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "idField": "_id",
                "sortDirection": 1,
                "limit": 500
            }
        }, {
            "sizex": 3,
            "type": "aggregationTable",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "groupField": "net",
                "aggregationType": "max",
                "aggregationField": "mag",
                "limit": 100
            }
        }, {
            "sizex": 3,
            "type": "barChart",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "groupField": "net",
                "aggregationType": "average",
                "aggregationField": "mag",
                "limit": 50
            }
        }, {
            "sizex": 3,
            "type": "newsFeed",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "primaryTitleField": "type",
                "secondaryTitleField": "net",
                "dateField": "time",
                "contentField": "mag"
            }
        }, {
            "sizex": 3,
            "type": "opsClock",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "dateField": "time"
            }
        }, {
            "sizex": 3,
            "type": "scatterPlot",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "type": "scatter",
                "xAxisField": "depth",
                "yAxisField": "mag",
                "limit": "time"
            }
        }, {
            "sizex": 3,
            "type": "sunburstChart",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "groupFields": "type1,type2,type3",
                "valueField": "type",
                "arcValue": "count"
            }
        }, {
            "sizex": 3,
            "type": "textCloud",
            "bindings": {
                "title": "myTitle",
                "database": "record",
                "table": "ebola_data",
                "dataField": "net"
            }
        }]
    },

