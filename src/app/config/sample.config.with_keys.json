{
  "datastores": {
    "es_1": {
      "host": "localhost",
      "type": "elasticsearch",
      "databases": {
        "earthquakes": {
          "prettyName": "Earthquakes Elasticsearch 6",
          "tables": {
            "quakedata": {
              "prettyName": "Earthquake Data",
              "fields": [
                {
                  "columnName": "id",
                  "prettyName": "ID"
                },
                {
                  "columnName": "depth",
                  "prettyName": "Depth"
                },
                {
                  "columnName": "latitude",
                  "prettyName": "Latitude"
                },
                {
                  "columnName": "longitude",
                  "prettyName": "Longitude"
                },
                {
                  "columnName": "mag",
                  "prettyName": "Magnitude"
                },
                {
                  "columnName": "magType",
                  "prettyName": "Magnitude Type"
                },
                {
                  "columnName": "net",
                  "prettyName": "Net"
                },
                {
                  "columnName": "time",
                  "prettyName": "Time"
                },
                {
                  "columnName": "type",
                  "prettyName": "Type"
                },
                {
                  "columnName": "_id",
                  "prettyName": "ES ID"
                }
              ]
            }
          }
        }
      }
    }
  },
  "dashboards": {
    "name": "Earthquakes Elasticsearch 6 Dashboard",
    "layout": "earthquakes_layout",
    "tables": {
      "table_key_1": "es_1.earthquakes.quakedata"
    },
    "fields": {
      "id_key": "es_1.earthquakes.quakedata.id",
      "depth_key": "es_1.earthquakes.quakedata.depth",
      "latitude_key": "es_1.earthquakes.quakedata.latitude",
      "longitude_key": "es_1.earthquakes.quakedata.longitude",
      "mag_key": "es_1.earthquakes.quakedata.mag",
      "mag_type_key": "es_1.earthquakes.quakedata.magType",
      "net_key": "es_1.earthquakes.quakedata.net",
      "time_key": "es_1.earthquakes.quakedata.time",
      "type_key": "es_1.earthquakes.quakedata.type",
      "_id_key": "es_1.earthquakes.quakedata._id"
    },
    "options": {
      "connectOnLoad": true
    }
  },
  "layouts": {
    "earthquakes_layout": [
      {
        "type": "aggregation",
        "col": 1,
        "row": 1,
        "sizex": 6,
        "sizey": 4,
        "bindings": {
          "title": "Timeline",
          "type": "histogram",
          "tableKey": "table_key_1",
          "xField": "time_key",
          "granularity": "dayOfMonth",
          "groupField": "net_key",
          "ignoreSelf": true,
          "limit": 100
        }
      },
      {
        "type": "aggregation",
        "col": 1,
        "row": 5,
        "sizex": 6,
        "sizey": 4,
        "bindings": {
          "title": "Networks",
          "type": "bar-h",
          "tableKey": "table_key_1",
          "xField": "net_key",
          "groupField": "net_key",
          "sortByAggregation": true,
          "ignoreSelf": true,
          "limit": 11
        }
      },
      {
        "type": "aggregation",
        "col": 1,
        "row": 9,
        "sizex": 6,
        "sizey": 4,
        "bindings": {
          "title": "Magnitudes",
          "type": "histogram",
          "tableKey": "table_key_1",
          "xField": "mag_key",
          "groupField": "net_key",
          "ignoreSelf": true,
          "limit": 100
        }
      },
      {
        "type": "aggregation",
        "col": 1,
        "row": 13,
        "sizex": 6,
        "sizey": 4,
        "bindings": {
          "title": "Depths",
          "type": "histogram",
          "tableKey": "table_key_1",
          "xField": "depth_key",
          "groupField": "net_key",
          "ignoreSelf": true,
          "limit": 100
        }
      },
      {
        "type": "map",
        "col": 7,
        "row": 1,
        "sizex": 6,
        "sizey": 10,
        "bindings": {
          "title": "Locations",
          "mapType": "leaflet",
          "tableKey": "table_key_1",
          "limit": 5000,
          "layers": [
            {
              "title": "Earthquakes",
              "tableKey": "table_key_1",
              "latitudeField": "latitude_key",
              "longitudeField": "longitude_key",
              "colorField": "net_key"
            }
          ]
        }
      },
      {
        "type": "dataTable",
        "col": 7,
        "row": 11,
        "sizex": 6,
        "sizey": 6,
        "bindings": {
          "title": "Raw Data",
          "tableKey": "table_key_1",
          "sortField": "time_key",
          "ignoreSelf": false,
          "limit": 100
        }
      }
    ]
  }
}
