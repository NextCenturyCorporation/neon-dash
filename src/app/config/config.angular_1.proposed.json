    "dashboard": {
        "gridsterColumns": 24,
        "gridsterMargins": 10,
        "hideNavbarItems": false,
        "hideAdvancedOptions": false,
        "hideAddVisualizationsButton": false,
        "hideErrorNotifications": true,
        "hideHeaders": false,
        "hideCloseButton": false,
        "showVideoOnLoad": false,
        "showImport": false,
        "showExport": true,
        "translationKeys": {
            "google": "API_KEY"
        }
