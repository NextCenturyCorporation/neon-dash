    "help": {
        "guide": "./app/help/Neon-Dashboard-User-Guide.pdf",
        "webVideo": "",
        "localVideo": ""
    },
    "opencpu": {
        "enableOpenCpu": false,
        "enableLogging": false,
        "useAlerts": false,
        "enableAnomalyDetection": true,
        "enableStl2": false,
        "enableMmpp": false,
        "url": "http://neon-opencpu/ocpu/library/NeonAngularDemo/R"
    },
    "externalServices": {
        "jsonConfigFile": "",
        "appImagePath": "",
        "fileProps": {
            "url": "url",
            "name": "name",
            "image": "image",
            "services": "services"
        },
        "servicesMappings": {
            "neonMapping1": "serviceName1",
