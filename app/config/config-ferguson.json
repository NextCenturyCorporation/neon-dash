    "layouts": {
        "default": [{
            "sizeX": 6,
            "sizeY": 1,
            "minSizeY": 1,
            "type": "timeline-selector"
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "map",
            "bindings": {
                "bind-config": "'myMap1'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "circular-heat-form"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "linechart"
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "filter-builder"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "scatterplot"
        }],

        "example-with-bindings": [{
            "sizeX": 6,
            "sizeY": 1,
            "type": "timeline-selector",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-date-field": "'date'",
                "override-start-date": "'1/1/2013'",
                "override-end-date": "1388534399000"
            }
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "map",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-latitude-field": "'latitude'",
                "bind-longitude-field": "'longitude'",
                "bind-color-field": "'type'",
                "bind-size-field": "'type'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "circular-heat-form",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-date-field": "'date'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "linechart",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-date-field": "'date'",
                "bind-y-axis-field": "'type'",
                "bind-category-field": "'type'",
                "bind-aggregation-field": "'count'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "scatterplot",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-x-axis-field": "'sentiment'",
                "bind-y-axis-field": "'arousal'"
            }
        },{
            "sizeX": 2,
            "sizeY": 2,
            "type": "barchart",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-x-axis-field": "'type'",
                "bind-y-axis-field": "'type'",
                "bind-aggregation-field": "'count'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "count-by",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-count-field": "'type'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "tag-cloud",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-tag-field": "'hashtags'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "sunburst",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 2,
            "type": "query-results-table",
            "bindings": {
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'"
            }
        }]
    },

