{
    "user_ale": {
        "loggingUrl": "http://192.168.1.100",
        "toolName": "Neon Dashboard",
        "toolVersion": "1.1.0",
        "elementGroups": [
            "top",
            "map_group",
            "table_group",
            "chart_group",
            "query_group",
            "graph_group"
        ],
        "workerUrl": "lib/user-ale/js/userale-worker.js",
        "debug": false,
        "sendLogs": false
    },

    "dashboard": {
        "gridsterColumns": 6,
        "gridsterMargins": 10,
        "hideNavbarItems": false,
        "hideAdvancedOptions": false,
        "hideErrorNotifications": false,
        "hideHeaders": false,
        "hideCloseButton": false,
        "showVideoOnLoad": false
    },

    "help": {
        "guide": "",
        "video": ""
    },

    "opencpu": {
        "enableOpenCpu": false,
        "enableLogging": false,
        "useAlerts": false,
        "enableAnomalyDetection": true,
        "enableStl2": false,
        "enableMmpp": false,
        "url": "http://neon-opencpu/ocpu/library/NeonAngularDemo/R"
    },

    "dig": {
        "enabled": false,
        "server": "http://localhost:9000"
    },

    "visualizations": [{
        "name": "Filter Builder",
        "sizeX": 4,
        "minSizeX": 3,
        "sizeY": 2,
        "minSizeY": 1,
        "type": "filter-builder",
        "icon": "img/visualizations/CreateFilter64.png"
    }, {
        "name": "Data Table",
        "sizeX": 6,
        "sizeY": 2,
        "type": "query-results-table",
        "icon": "img/visualizations/ViewData64.png"
    }, {
        "name": "Timeline",
        "sizeX": 6,
        "sizeY": 1,
        "minSizeY": 1,
        "type": "timeline-selector",
        "icon": "img/visualizations/Timeline64.png"
    }, {
        "name": "Map",
        "sizeX": 4,
        "sizeY": 2,
        "type": "map",
        "icon": "img/visualizations/Map64.png"
    }, {
        "name": "Ops Clock",
        "sizeX": 2,
        "sizeY": 2,
        "type": "circular-heat-form",
        "icon": "img/visualizations/OpsClock64.png"
    }, {
        "name": "Line Chart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "linechart",
        "icon": "img/visualizations/LineChart64.png"
    }, {
        "name": "Bar Chart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "barchart",
        "icon": "img/visualizations/BarChart64.png"
    }, {
        "name": "Sunburst Chart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "sunburst",
        "icon": "img/visualizations/Sunburst64.png"
    }, {
        "name": "Text Cloud",
        "sizeX": 2,
        "sizeY": 2,
        "type": "tag-cloud",
        "icon": "img/visualizations/TagCloud64.png"
    }, {
        "name": "Aggregation Table",
        "sizeX": 2,
        "sizeY": 2,
        "type": "count-by",
        "icon": "img/visualizations/Count64.png"
    }, {
        "name": "Graph",
        "sizeX": 4,
        "sizeY": 2,
        "type": "directed-graph",
        "icon": "img/visualizations/Graph64.png"
    },{
        "name": "Scatterplot",
        "sizeX": 2,
        "sizeY": 2,
        "type": "scatterplot",
        "icon": "img/visualizations/LineChart64.png"
    }],

    "datasets": [{
        "name": "DC2014",
        "datastore": "mongo",
        "hostname": "localhost",
        "databases": [{
            "name": "dc2014",
            "prettyName": "DC 2014",
            "tables": [{
                "name": "activity",
                "prettyName": "Activity",
                "fields": [{
                    "columnName": "_id",
                    "prettyName": "Database ID"
                }, {
                    "columnName": "time",
                    "prettyName": "Time"
                }, {
                    "columnName": "screen_name",
                    "prettyName": "Screen Name"
                }, {
                    "columnName": "user_followers_count",
                    "prettyName": "Number of Followers"
                }, {
                    "columnName": "user_friends_count",
                    "prettyName": "Number of Friends"
                }, {
                    "columnName": "lang",
                    "prettyName": "Language"
                }, {
                    "columnName": "keywords",
                    "prettyName": "Keywords"
                }, {
                    "columnName": "mentions",
                    "prettyName": "Mentions"
                }, {
                    "columnName": "retweet_count",
                    "prettyName": "Number of Retweets"
                }, {
                    "columnName": "text",
                    "prettyName": "Text"
                }, {
                    "columnName": "longitude",
                    "prettyName": "Longitude"
                }, {
                    "columnName": "latitude",
                    "prettyName": "Latitude"
                }, {
                    "columnName": "text_array",
                    "prettyName": "Text Array"
                }, {
                    "columnName": "type",
                    "prettyName": "Activity Type"
                }],
                "mappings": {
                    "date": "time",
                    "latitude": "latitude",
                    "longitude": "longitude",
                    "line_category": "type",
                    "bar_x_axis": "net",
                    "y_axis": "mag",
                    "color_by": "type",
                    "size_by": "type",
                    "sort_by": "created_at",
                    "count_by": "net"
                }
            }]
        }],
        "mapConfig": {
            "bounds" : {
                "left": -180,
                "bottom": -90,
                "right": 180,
                "top": 90
            }
        },
        "mapLayers": [{
            "database": "dc2014",
            "table": "activity",
            "type": "heatmap",
            "latitudeMapping": "latitude",
            "longitudeMapping": "longitude",
            "limit": 5000,
            "sizeBy": "retweet_count"
        }, {
            "database": "dc2014",
            "table": "activity",
            "active":  true,
            "type": "cluster",
            "latitudeMapping": "latitude",
            "longitudeMapping": "longitude",
            "limit": 5000,
            "colorBy": "type",
            "sizeBy": "retweet_count",
            "linkyConfig": {
              "mentions": true,
              "hashtags": true,
              "urls": false,
              "linkTo": "twitter"
            }
        }, {
            "database": "dc2014",
            "table": "activity",
            "active":  true,
            "type": "points",
            "latitudeMapping": "latitude",
            "longitudeMapping": "longitude",
            "limit": 5000,
            "colorBy": "type",
            "sizeBy": "retweet_count",
            "linkyConfig": {
              "mentions": true,
              "hashtags": true,
              "urls": false,
              "linkTo": "twitter"
            }
        }]
    }
