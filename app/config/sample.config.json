{
    "datasets": [{
        "name": "Earthquakes",
        "datastore": "mongo",
        "hostname": "localhost",
        "layout": "earthquakes",
        "connectOnLoad": true,
        "databases": [{
            "name": "test",
            "prettyName": "Test",
            "tables": [{
                "name": "earthquakes",
                "prettyName": "Earthquakes",
                "fields": [{
                    "columnName": "_id",
                    "prettyName": "Database ID"
                }, {
                    "columnName": "time",
                    "prettyName": "Time"
                }, {
                    "columnName": "latitude",
                    "prettyName": "Latitude"
                }, {
                    "columnName": "longitude",
                    "prettyName": "Longitude"
                }, {
                    "columnName": "depth",
                    "prettyName": "Depth"
                }, {
                    "columnName": "mag",
                    "prettyName": "Magnitude"
                }, {
                    "columnName": "magType",
                    "prettyName": "Magnitude Type"
                }, {
                    "columnName": "nst",
                    "prettyName": "NST"
                }, {
                    "columnName": "gap",
                    "prettyName": "Gap"
                }, {
                    "columnName": "dmin",
                    "prettyName": "DMIN"
                }, {
                    "columnName": "rms",
                    "prettyName": "RMS"
                }, {
                    "columnName": "net",
                    "prettyName": "Net"
                }, {
                    "columnName": "id",
                    "prettyName": "ID"
                }, {
                    "columnName": "updated",
                    "prettyName": "Updated"
                }, {
                    "columnName": "place",
                    "prettyName": "Place"
                }, {
                    "columnName": "type",
                    "prettyName": "Type"
                }],
                "mappings": {
                    "date": "time",
                    "latitude": "latitude",
                    "longitude": "longitude",
                    "line_category": "type",
                    "bar_x_axis": "net",
                    "y_axis": "mag",
                    "color_by": "magType",
                    "size_by": "mag",
                    "sort_by": "time",
                    "count_by": "net",
                    "allCoordinates": [{
                        "latitude": "latitude",
                        "longitude": "longitude"
                    }]
                }
            }]
        }],
        "mapLayers": {
            "myMap1": [{
                "database": "test",
                "table": "earthquakes",
                "type": "heatmap",
                "latitudeMapping": "latitude",
                "longitudeMapping": "longitude",
                "limit": 5000,
                "sizeBy": "mag"
            }, {
                "database": "test",
                "table": "earthquakes",
                "active":  true,
                "type": "cluster",
                "latitudeMapping": "latitude",
                "longitudeMapping": "longitude",
                "limit": 5000,
                "colorBy": "magType",
                "sizeBy": "mag"
            }, {
                "database": "test",
                "table": "earthquakes",
                "active":  true,
                "type": "points",
                "latitudeMapping": "latitude",
                "longitudeMapping": "longitude",
                "limit": 5000,
                "colorBy": "magType",
                "sizeBy": "mag"
            }]
        },
        "lineCharts": {
            "lineChart1": [
                {
                    "database": "test",
                    "table": "earthquakes",
                    "active":  true,
                    "name": "Quakes",
                    "xAxis": "time",
                    "aggregation": "sum",
                    "yAxis": "mag",
                    "category": "magType"
                },{
                    "database": "test",
                    "table": "earthquakes",
                    "active":  true,
                    "name": "Quakes2",
                    "xAxis": "time",
                    "aggregation": "count"
                }
            ],
            "lineChart2": [
                {
                    "database": "test",
                    "table": "earthquakes",
                    "active":  true,
                    "name": "Quakes",
                    "xAxis": "time",
                    "aggregation": "max",
                    "yAxis": "mag"
                }
            ]
        },
        "mapConfig": {
            "myMap1": {
                "bounds" : {
                    "top": 90,
                    "bottom": -90,
                    "left": -180,
                    "right": 180
                }
            }
        },
        "linkyConfig": {
          "mentions": false,
          "hashtags": false,
          "urls": true,
          "linkTo": "twitter"
        },
        "options": {
            "colorMaps": {
                "test": {
                    "earthquakes": {
                        "myField": {
                            "fieldA": "#123456",
                            "fieldB": "#654321"
                        }
                    }
                }
            },
            "mapBaseLayer": {
                "color": "dark",
                "protocol": "https"
            }
        }
    }],

    "layouts": {

        "default": [{
            "sizeX": 12,
            "minSizeX": 8,
            "sizeY": 7,
            "minSizeY": 4,
            "type": "timeline-selector"
        }, {
            "sizeX": 12,
            "sizeY": 7,
            "type": "map",
            "bindings": {
                "bind-config": "'myMap1'"
            }
        }, {
            "sizeX": 12,
            "sizeY": 6,
            "type": "query-results-table"
        }, {
            "sizeX": 12,
            "sizeY": 6,
            "type": "filter-builder"
        }],

        "earthquakes": [{
            "sizeX": 24,
            "minSizeX": 12,
            "sizeY": 4,
            "minSizeY": 4,
            "type": "timeline-selector"
        }, {
            "sizeX": 12,
            "sizeY": 8,
            "type": "map",
            "bindings": {
                "bind-config": "'myMap1'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "circular-heat-form"
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "linechart",
            "bindings": {
                "bind-config": "'lineChart1'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "barchart"
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "count-by"
        }, {
            "sizeX": 18,
            "sizeY": 4,
            "type": "query-results-table"
        }, {
            "sizeX": 6,
            "sizeY": 6,
            "type": "plotly-graph",
            "bindings": {
                "bind-attr-x": "'depth'",
                "bind-attr-y": "'mag'"
            }
        }, {
            "sizeX": 18,
            "sizeY": 4,
            "type": "filter-builder"
        }],

        "example-with-bindings": [{
            "sizeX": 12,
            "sizeY": 4,
            "type": "timeline-selector",
            "bindings": {
                "bind-title": "'myTitle'",
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-date-field": "'date'",
                "override-start-date": "'1/1/2013'",
                "override-end-date": "1388534399000"
            }
        }, {
            "sizeX": 12,
            "sizeY": 4,
            "type": "map",
            "bindings": {
                "bind-config": "'myMap1'",
                "bind-title": "'myTitle'",
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-latitude-field": "'latitude'",
                "bind-longitude-field": "'longitude'",
                "bind-color-field": "'type'",
                "bind-size-field": "'type'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "circular-heat-form",
            "bindings": {
                "bind-title": "'myTitle'",
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-date-field": "'date'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "linechart",
            "bindings": {
                "bind-config": "'myLineChart'",
                "bind-granularity": "'day'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "barchart",
            "bindings": {
                "bind-title": "'myTitle'",
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-x-axis-field": "'type'",
                "bind-y-axis-field": "'type'",
                "bind-aggregation-field": "'count'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "count-by",
            "bindings": {
                "bind-title": "'myTitle'",
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-count-field": "'type'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "tag-cloud",
            "bindings": {
                "bind-title": "'myTitle'",
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'",
                "bind-tag-field": "'hashtags'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "sunburst",
            "bindings": {
                "bind-title": "'myTitle'",
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'"
            }
        }, {
            "sizeX": 12,
            "sizeY": 4,
            "type": "query-results-table",
            "bindings": {
                "bind-title": "'myTitle'",
                "bind-database": "'myDatabase'",
                "bind-table": "'myTable'"
            }
        }]
    },

